<%= render '/shared/header' %>
    <% @discusses.each do |discuss| %>
      <div>
        <td><%= link_to "#{discuss.title}", forum_subject_topic_discuss_posts_path(discuss_id: discuss.id) %></td>
        <td><%= discuss.description %></td>
        <td><%= link_to 'Show', forum_subject_topic_discuss_path(id: discuss.id) %></td>
        <td><%= link_to 'Edit', edit_forum_subject_topic_discuss_path(id: discuss.id) %></td>
        <td><%= link_to 'Destroy', discuss, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </div>
      </br>
    <% end %>
    <%= link_to 'New Discuss', new_forum_subject_topic_discuss_path %>
  </div>
</body>
