<h1 id="banner">Banner</h1>

<body class="wrapper">
<div class="left_bar">Left</div>
  <div class="right_bar">Right Bar</div>
  <div class="middle_content">
    <% @forums.each do |forum| %>
      <div>
        <td class="forum_title"><%= link_to "#{forum.title}", forum_subjects_path(forum_id: forum.id) %></td>
        <td><%= forum.description %></td>
        <td><%= link_to 'Show', forum %></td>
        <td><%= link_to 'Edit', edit_forum_path(forum) %></td>
        <td><%= link_to 'Destroy', forum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </div>
      </br>
    <% end %>
    <%= link_to 'New Forum', new_forum_path %>
  </div>
</body>

